{
  "name": "react-native-workshop",
  "version": "0.0.0",
  "devDependencies": {
    "react-scripts": "0.6.1",
    "sw-precache": "^5.1.0"
  },
  "dependencies": {
    "highlight.js": "9.7.0",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-router": "2.8.1",
    "react-syntax-highlight": "0.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && sw-precache --root='build/' --static-file-globs='build/**/!(*map*)' --verbose",
    "test": "react-scripts test --env=jsdom",
    "stage": "npm run build && surge build -d react-native-workshop.surge.sh"
  },
  "homepage": "./"
}
