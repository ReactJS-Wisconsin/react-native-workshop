{
  "name": "react-native-workshop",
  "version": "0.0.0",
  "devDependencies": {
    "react-scripts": "0.9.5",
    "sw-precache": "^5.1.0"
  },
  "dependencies": {
    "highlight.js": "9.10.0",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-router": "2.8.1",
    "react-syntax-highlight": "0.0.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && sw-precache --root='build/' --static-file-globs='build/**/!(*map*)' --verbose",
    "test": "react-scripts test --env=jsdom",
    "deploy": "npm run build && surge build -d react-native-workshop.surge.sh",
    "teardown": "surge teardown react-native-workshop.surge.sh"
  },
  "homepage": "./"
}
